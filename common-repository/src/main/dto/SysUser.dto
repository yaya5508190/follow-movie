export com.yx.nas.entity.SysUser
    -> package com.yx.nas.dto

input SysUserInput {
    #allScalars(SysUser)
    -id
}

input SysUserUpdateInput {
    id
    nickName
    email
    enabled
    fuzzy password?
}

specification SysUserSpec {
    like/i(userName)
    like/i(nickName)
    like/i(email)
    eq(enabled)
}

SysUserView {
    #allScalars(this)
    -password
    createTime
    updateTime
}

SysUserDetailView {
    #allScalars(this)
}

